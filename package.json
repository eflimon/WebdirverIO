{
  "name": "webdriverio-framework",
  "version": "1.0.0",
  "description": "webdriverio automation framework",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "wdio": "wdio run ./wdio.conf.js",

    "smoke-suite": "npx wdio --suite smoke",
    
